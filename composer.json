{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Online Tours. A project for tourism in Egypt.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "alexazartsev/heroicon": "^1.0",
        "barryvdh/laravel-dompdf": "^2.2",
        "benjacho/belongs-to-many-field": "^2.0",
        "benjaminhirsch/nova-slug-field": "^1.3",
        "biscolab/laravel-recaptcha": "^5.4",
        "chris-ware/nova-breadcrumbs": "^2.3",
        "classic-o/nova-media-library": "^1.0",
        "codenco-dev/nova-grid-system": "^1.0",
        "crayon/nova-language-management": "^1.0",
        "davidpiesse/nova-toggle": "^0.0.8",
        "digital-creative/collapsible-resource-manager": "^1.4",
        "dillingham/nova-ajax-select": "^1.0",
        "dillingham/nova-attach-many": "v1.3.0",
        "dillingham/nova-button": "^1.0",
        "dioscouri/omnipay-cybersource": "^1.0",
        "dmitrybubyakin/nova-medialibrary-field": "^3.3",
        "dyrynda/laravel-cascade-soft-deletes": "^4.2",
        "epartment/nova-dependency-container": "^1.3",
        "fruitcake/laravel-cors": "^2.0",
        "google/recaptcha": "~1.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "jenssegers/agent": "^2.6",
        "kalnoy/nestedset": "^6.0",
        "laravel/framework": "^8.75",
        "laravel/nova": "*",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "league/omnipay": "^3.2",
        "maatwebsite/laravel-nova-excel": "1.2.*",
        "mdixon18/fontawesome": "^0.2.1",
        "mostafaznv/nova-ckeditor": "^1.0",
        "mostafaznv/nova-video": "^1.0",
        "nova-fields/rating": "^1.0",
        "optimistdigital/nova-multiselect-field": "^2.4",
        "optimistdigital/nova-settings": "3.*",
        "optimistdigital/nova-sortable": "v2.4.4",
        "outl1ne/nova-simple-repeatable": "^1.4",
        "rmsj/nova-hero-icon": "^1.0",
        "sbine/route-viewer": "^0.0.7",
        "spatie/laravel-query-builder": "^4.0",
        "spatie/laravel-sitemap": "^5.9",
        "spatie/nova-backup-tool": "4.*",
        "spatie/nova-tags-field": "v3.4.5",
        "superlatif/nova-tag-input": "^1.4",
        "vyuldashev/nova-permission": "^3.0",
        "waynestate/nova-ckeditor4-field": "^0.7.0",
        "whitecube/nova-flexible-content": "^0.2.14",
        "yassi/nova-nested-form": "^3.0",
        "ziffdavis/nova-nestedset": "^0.0.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Files/baseHelpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
